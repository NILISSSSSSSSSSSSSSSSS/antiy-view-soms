{
  "private": true,
  "scripts": {
    "start": "cross-env PORT=8080 roadhog server",
    "build": "cross-env roadhog build",
    "lint": "eslint --ext .js src test",
    "test": "rimraf coverage && cross-env NODE_ENV=test jest",
    "test:coverage": "rimraf coverage && cross-env NODE_ENV=test jest --coverage || exit 0",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "npm run lint",
    "analyze": "cross-env ANALYZE=1 roadhog build"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/enzyme.config.js"
    ],
    "setupFiles": [
      "mock-local-storage"
    ],
    "collectCoverage": false,
    "globals": {
      "CONFIG_ENV": true
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.spec.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 60,
        "branches": 60,
        "functions": 60,
        "lines": 60
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/testBuild/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/testBuild/jest/fileTransform.js"
    },
    "moduleNameMapper": {
      "\\.(local.less)$": "identity-obj-proxy",
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/testBuild/jest/styleMock.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)": "<rootDir>/testBuild/jest/fileMock.js",
      "@/": "<rootDir>/src/",
      "@/a": "<rootDir>/src/assets",
      "@/c": "<rootDir>/src/components",
      "@/u": "<rootDir>/src/utils"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "src/.*\\.spec\\.js$"
  },
  "dependencies": {
    "antd": "^3.18.2",
    "axios": "^0.19.0",
    "babel-plugin-import": "^1.11.0",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.0.1",
    "dva": "^2.4.1",
    "dva-loading": "^3.0.4",
    "echarts": "^4.2.0-rc.2",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.13",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-dva-hmr": "^0.3.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "crypto-js": "^3.1.9-1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^4.14.0",
    "eslint-config-umi": "^0.1.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.12.0",
    "jasmine-core": "^3.5.0",
    "jest": "^24.9.0",
    "mock-local-storage": "^1.1.8",
    "redbox-react": "^1.4.3",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^3.0.0",
    "roadhog": "^2.5.0-beta.1",
    "sinon": "^7.5.0"
  }
}
